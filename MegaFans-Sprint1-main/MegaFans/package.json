{
  "name": "megafans",
  "version": "1.0.0",
  "description": "This is HardHat Environment for MegaFans Project",
  "scripts": {
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "test": "npx hardhat test",
    "test-ganache": "npx hardhat test --network ganache",
    "test-mumbai": "npx hardhat test --network mumbai_testnet",
    "test-polygon": "npx hardhat test --network polygon_mainnet",
    "deploy": "npx hardhat run scripts/deploy.js",
    "deploy-ganache": "npx hardhat run scripts/deploy.js --network ganache",
    "deploy-mumbai": "npx hardhat run scripts/deploy.js --network mumbai_testnet",
    "deploy-polygon": "npx hardhat run scripts/deploy.js --network polygon_mainnet",
    "console": "npx hardhat console",
    "console-ganache": "npx hardhat console --network ganache",
    "console-mumbai": "npx hardhat console --network mumbai_testnet",
    "console-polygon": "npx hardhat console --network polygon_mainnet"
  },
  "keywords": [],
